  "description": "React Quiz Component",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "docs": "webpack --mode production",
